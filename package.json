{
  "name": "koilang",
  "displayName": "KoiLang",
  "description": "Syntax highlighting and language support for KoiLang",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "koilang",
        "aliases": [
          "KoiLang",
          "koilang"
        ],
        "extensions": [
          ".koi"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "koilang",
        "scopeName": "source.koi",
        "path": "./syntaxes/koilang.json"
      }
    ]
  }
}